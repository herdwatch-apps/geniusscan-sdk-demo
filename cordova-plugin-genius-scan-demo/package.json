{
  "name": "com.geniusscansdk.cordova.demo",
  "version": "1.0.0",
  "displayName": "Cordova Demo",
  "scripts": {
    "clean:up": "rm -rf {platforms,plugins}",
    "add:ios": "cordova platform add ios",
    "build:ios:local": "cordova build ios --release --device -- --automaticProvisioning=true"
  },
  "devDependencies": {
    "@thegrizzlylabs/cordova-plugin-genius-scan": "5.3.0",
    "cordova-android": "~13.0.0",
    "cordova-ios": "7.1.0",
    "cordova-plugin-file": "8.1.0",
    "cordova-plugin-firebasex": "^17.0.0",
    "cordova-plugin-preview-any-file": "^0.2.9"
  },
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-plugin-file": {},
      "cordova-plugin-preview-any-file": {},
      "@thegrizzlylabs/cordova-plugin-genius-scan": {
        "CAMERA_USAGE_DESCRIPTION": "Access camera for demo"
      },
      "cordova-plugin-firebasex": {}
    }
  }
}